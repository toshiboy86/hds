<assembly xmlns="http://maven.apache.org/ASSEMBLY/2.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/ASSEMBLY/2.0.0 http://maven.apache.org/xsd/assembly-2.0.0.xsd">
	<id>hds-playbook</id>
	<formats>
		<format>tar.gz</format>
	</formats>
	<baseDirectory>hds-playbook</baseDirectory>
	<includeBaseDirectory>true</includeBaseDirectory>
	<moduleSets>
		<moduleSet>
			<useAllReactorProjects>true</useAllReactorProjects>
			<includes>
				<include>jp.ciof_cps:ciof-hds-dev</include>
			</includes>
			<sources>
				<fileSets>
					<fileSet>
						<outputDirectory>roles/ciofuserdb/files</outputDirectory>
						<includes>
							<include>ciofuserdb.*.sql</include>
						</includes>
						<directory>src/main/sql</directory>
					</fileSet>
					<fileSet>
						<outputDirectory>roles/ciofhdsdb/files</outputDirectory>
						<includes>
							<include>ciofhdsdb.*.sql</include>
						</includes>
						<directory>src/main/sql</directory>
					</fileSet>
				</fileSets>
				<includeModuleDirectory>false</includeModuleDirectory>
			</sources>
			<binaries>
				<outputDirectory>roles/ciofhdswar/files</outputDirectory>
				<includeDependencies>false</includeDependencies>
				<includes>
					<include>jp.ciof_cps:ciof-hds-dev:war:*</include>
				</includes>
				<unpack>false</unpack>
				<outputFileNameMapping>${module.build.finalName}.${module.extension}</outputFileNameMapping>
			</binaries>
		</moduleSet>
	</moduleSets>
	<fileSets>
		<fileSet>
			<directory>hds-playbook</directory>
			<outputDirectory></outputDirectory>
		</fileSet>
	</fileSets>
</assembly>
